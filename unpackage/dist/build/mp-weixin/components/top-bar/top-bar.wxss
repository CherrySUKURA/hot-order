.topbar{width:100%;height:200rpx;background-color:#0448ab;position:fixed;top:0;left:0;box-shadow:0rpx 7rpx 7rpx -7rpx hsla(0,0%,100%,.5);z-index:2}.topbar .barBtn{width:50rpx;height:40rpx;position:absolute;bottom:55rpx;cursor:pointer}.topText{font-size:40rpx;font-weight:400;color:#fff;position:absolute;bottom:52rpx;left:50%;-webkit-transform:translateX(-50%);transform:translateX(-50%)}.leftBtn{left:40rpx}.rightBtn{right:40rpx}